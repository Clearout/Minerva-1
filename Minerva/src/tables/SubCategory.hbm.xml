<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="src.tables">
    <class name="SubCategory" table="sub_category">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <many-to-one name="category" column="id" not-null="true"/>
    </class>
</hibernate-mapping>
        